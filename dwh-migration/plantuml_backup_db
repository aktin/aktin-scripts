@startuml
start
:Copy "create_backup" script to old server;

partition "Old Ubuntu Server" {
    :Move to "create_dwh_backup.sh";
    :Execute "create_dwh_backup.sh";

    rectangle "create_dwh_backup.sh" {
        :Start backup;
        :"backup_file.tar";
        :Copy "backup_file.tar" to new Server;
    }

}

partition "New Server" {
    :Copy "apply_dwh_backup" script to "backup_file.tar" dir;
    :Execute "apply_dwh_backup" script;
    :Test DWH if okay;
}

end
@enduml


'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

@startuml
start
partition "Create Backup of Docker DWH  " {
    :Display running docker containers;
    :**Prompt:** Insert target wildfly container;
    :Await input;
    note left: **var:** wildfly_container_name

    if (Container found?) then (yes)

    else (no)
        :"Container not found" Error;
        end
    endif

}

partition "create_dwh_backup" {
    :crate local dwh backup;
}
end
@enduml


