@startuml
actor Promoter
actor Entrant

Promoter --> (Create Event)
Promoter -> (Attend Event)

Entrant --> (Find Event)
(Attend Event) <- Entrant

(Attend Event) <.. (Create Member)  : <<include>>

@enduml





@startuml
skinparam dpi 180

' Local
top to bottom direction
rectangle "Diagram A" {
  actor User
  User --> (Login)
  (Login) --> (Dashboard)
}

rectangle "Diagram B" {
  actor Admin
  Admin --> (Review)
  (Review) --> (Approve)
}
@enduml


